

<view class="root">


<view class="state">
  
  <image src="../../static/weita/state.jpg"></image>

</view>
<view class="lend">
<image src="../../static/weita/get.jpg" bindtap="quhuo"></image>

</view>
<view class="return">
  <image src="../../static/weita/return.jpg"></image>
</view>
<view class="answer">
  <image src="../../static/weita/ans.jpg"></image>
</view>

<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item class="reset"  bindtap="toshop">
    <image
      slot="icon"
      src="{{ icon.normal }}"
      mode="aspectFit"
      style="width: 50px; height: 40px;"
    />
    <image
      slot="icon-active"
      src="{{ icon.active }}"
      mode="aspectFit"
      style="width: 50px; height: 40px;"
    />
  
  </van-tabbar-item>
  <van-tabbar-item class="reset" bindtap="towei">
    <image
      slot="icon"
      src="{{ icon.icon2 }}"
      mode="aspectFit"
      style="width: 60px; height: 60px;"
    />
    <image
      slot="icon-active"
      src="{{ icon.icon2 }}"
      mode="aspectFit"
      style="width: 60px; height: 60px;"
    />

  </van-tabbar-item>
  <van-tabbar-item class="reset" bindtap="tomine" >
    <image
      slot="icon"
      src="{{ icon.icon3 }}"
      mode="aspectFit"
      style="width: 50px; height: 40px;"
    />
    <image
      slot="icon-active"
      src="{{ icon.icon3 }}"
      mode="aspectFit"
      style="width: 50px; height: 40px;"
    />



  </van-tabbar-item>
</van-tabbar>

</view>